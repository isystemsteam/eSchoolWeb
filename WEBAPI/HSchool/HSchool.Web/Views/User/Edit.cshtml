@model HSchool.Business.Models.UserCreateModel
<div class="col-md-12 container">
    @using (Html.BeginForm("Edit", "User", FormMethod.Post, new { @Id = "formUser", @name = "formUser" }))
    {
        @Html.AntiForgeryToken()
        <h2>Create Internal User</h2>
        @Html.Partial("_CreateEditUser", Model)
        @Html.Partial("_AddressView", Model)
        <div class="row custom-row clearfix">
            <div class="col-lg-12 custom-check">
                <div class="cstom-check-margin">
                    <input type="checkbox" name="appAcceptTerms" id="userLoginEnabled" />
                    <label for="userLoginEnabled"><span><span><i class="fa fa-check"></i></span></span>Login Enabled </label>
                </div>
            </div>
        </div>
        <div class="row custom-row clearfix">
            <div class="col-md-12 custom-text">
                <input type="button" value="Save" class="btn-blue">
            </div>
        </div>
    }
</div>
@Scripts.Render("~/Scripts/Application/InternalUser.js")


